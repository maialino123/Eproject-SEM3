
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="Description" content="Enter your description here" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootswatch/4.6.0/sketchy/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <link href="~/assets/web/signIn/signIn.css" rel="stylesheet" />
    <title>Sign Up</title>
</head>
<body>

    <!-- Modal -->
    <div class="modal fade" id="otpEmail" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-4">
                            <label for="OtpResetpassword" style="width: 100%">CODE : </label>
                        </div>
                        <div class="col-md-8">
                            <input type="text" name="OtpResetpassword" id="OtpResetpassword" /> <span class="countdown" style="position: absolute; top: 10px; right: 10px; color: #311b92; border-left: 1px solid #616161 "></span>
                            <p id="txtErrorOtpResetpassword"></p>
                            <button class="sendAgain btn btn-default" style="background: blue; color: #fafafa; border-radius: 6px; width: 120px; margin-top: 20px">
                                send again
                            </button>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <nav class="navbar navbar-expand-lg navbar-light bg-light" style="background-color: #A1887F; height: 70px;">
                    <ul class="nav ">
                        <li class="nav-item">
                            <a class="nav-link active" href="#Home" style="font-size: 25px;" id="redirectHome">Flower</a>
                        </li>
                        <div class="row" style="position: absolute; right: 100px">
                            <li class="nav-item" style="margin-right: 10px;">
                                <input type="text" class="form-control" placeholder="Email" id="emailLogin">
                            </li>
                            <li class="nav-item" style="margin-right: 10px;">
                                <input type="password" class="form-control" placeholder="Password" id="passwordLogin">
                            </li>
                            <li class="nav-item" style="margin-right: 10px;">
                                <button class="btn" style="background-color: blue; width: 100px;" id="loginSubmit">Login</button>
                            </li>
                            <li class="nav-item" style="margin-right: 10px; margin-top: 5px; color: blue !important; font-size: 18px;">
                                <a href="#forgotPassword" id="redirectForgotPassword">You forgot your password?</a>
                            </li>
                        </div>
                    </ul>
                </nav>
            </div>
        </div>
        <div class="row mt-5">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <div class="content">
                    <div class="contentHeader">
                        <h2>Register</h2>
                    </div>
                    <div class="row mt-3">
                        <div class="col-md-6" id="animationName">
                            <Label for="inputfirstName">First Name</Label>
                            <input type="text" class="" id="firstName" placeholder="Enter your First Name">
                            <p class="Error" id="firstNameError">erorr</p>
                        </div>
                        <div class="col-md-6" id="animationLastName">
                            <Label for="inputLasttName">Last Name</Label>
                            <input type="text" class="" id="LastName" placeholder="Enter your Last Name">
                            <p class="Error" id="lastNameError">erorr</p>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-md-6" id="animationEmail">
                            <label for="inputEmail">Email</label>
                            <input type="text" class="" id="email" placeholder="Enter your Email">
                            <p class="Error" id="emailError">erorr</p>
                        </div>
                        <div class="col-md-6" id="animationPhone">
                            <label for="inputPhoneNumber">Phone Number</label>
                            <input type="text" class="" id="phoneNumber" placeholder="Enter your Phone number">
                            <p class="Error" id="phoneNumberError">erorr</p>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-md-6" id="animationBirthday">
                            <label for="inpuBirthDay">Birth Day</label>
                            <input type="Date" class="" id="birthDay">
                        </div>
                        <div class="col-md-6" id="animationGender">
                            <label for="inputRetypePassword">Gender</label>
                            <Select id="gender">
                                <option value="">Gender</option>
                                <option value="1">Male</option>
                                <option value="0">Female</option>
                            </Select>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-md-6" id="animationPassword">
                            <label for="inputPassword">Password</label>
                            <input type="password" class="" id="password" placeholder="Enter your Password">
                            <p class="Error" id="passwordError">erorr</p>
                        </div>
                        <div class="col-md-6" id="animationRetype">
                            <label for="inputRetypePassword">Retype Password</label>
                            <input type="password" class="" id="reTypePassword" placeholder="Retype your password">
                            <p class="Error" id="retypePasswordError">erorr</p>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-md-12" id="animationAddress">
                            <label for="inputAddress">Address</label>
                            <input type="text" class="" id="address" placeholder="Enter your Address">
                            <p class="Error" id="addressError">erorr</p>
                        </div>
                    </div>

                    <div class="row mt-4">
                        <div class="col-md-12" id="animationSignIn">
                            <button class="btn" id="btnSignIn" style="float: right;width: 100px; background-color: #0277bd; border-radius: 8px; padding: 8px; color: #fafafa">SignUp</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-3"></div>
        </div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/assets/admin/bootstrap4/js/bootstrap.js"></script>
    <script type="text/javascript" src="~/assets/admin/js/InnerFormValidation.js"></script>
    <script src="~/assets/admin/MDBBoostrap/fontAll.min.js"></script>
    <link href="~/assets/admin/admin-sbStyle/build/toastr.css" rel="stylesheet" type="text/css" />
    <!-- Async script executes immediately and must be after any DOM elements used in callback. -->
    <script src="~/assets/admin/admin-sbStyle/build/toastr.js"></script>ts used in callback. -->
    <script src="~/assets/admin/admin-sbStyle/build/toastr.js"></script>
    <script src="~/assets/web/signIn/signIn.js"></script>
</body>
</html>
